<div class="container" id="gameSearchContainer">
  {{#each games}}
  <a href="/game/id/{{id}}">
    <div class="card d-flex flex-row" id="gameSearchCard">
      <div class="col-2"><img src="https://images.igdb.com/igdb/image/upload/t_cover_med/{{cover.image_id}}.jpg"
          class="gameImage">
      </div>
      <div class="col-10">
        <h3>{{name}}</h3>
        <p>{{summary}}</p>
      </div>
    </div>
  </a>
  {{/each}}
</div>