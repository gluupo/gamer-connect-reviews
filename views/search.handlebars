<div class="container" id="gameSearchContainer">
  {{log this}}
  {{#each games}}
  <a href="/game/id/{{id}}">
    <div class="card" id="gameSearchCard">
      <h3>{{name}}</h3>
      <img src="https://images.igdb.com/igdb/image/upload/t_cover_med/{{cover.image_id}}.jpg" class="gameImage">
      <p>{{summary}}</p>
    </div>
  </a>
  {{/each}}
</div>